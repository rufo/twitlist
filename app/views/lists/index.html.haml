#users
  %ul
    - @users.each do |user|
      %li= image_tag user.profile_image_url, :title => user.name, :size => "48x48"

#lists
  %ul
    - @lists.collection.each do |list|
      %li
        = list.name
        %ul
          - @twitter.list_members(list.id).users.each do |list_member|
            %li= image_tag list_member.profile_image_url, :title => list_member.name, :size => "48x48"
